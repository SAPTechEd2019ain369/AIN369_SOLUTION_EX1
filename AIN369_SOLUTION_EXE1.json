			"properties": [
                "measureMax",
				"measureMin",
				"angleMax",
                "angleMin",
                "eventEffects"
			],
			"integrity": "",
            "ignoreIntegrity" : true
		}
    ],
	"properties": {
		"measureMax": {
			"type": "number",
			"description": "The max value the measure can take"
		},
		"measureMin": {
            "type": "number",
            "description": "The min value the measure can take"
		},
		"angleMax": {
            "type": "number",
            "description": "The resting angle of the gauge"
		},
		"angleMin": {
            "type": "number",
            "description": "The max anfgle of the gauge"
		},
		"measureVal": {
            "type": "number",
            "description": "Where the needle should be"
        },
        "eventEffects":{}
	},
	"methods": {
		"updateDrawing": {
			"description": "Update the gauge",
			"parameters": [
                {
					"name": "measureMin",
					"type": "number",
					"description": "Minim Measure Value, at which the gauge is in the rest position"
				},
				{
					"name": "measureMax",
					"type": "number",
					"description": "Maximum Measure Value, at which the gauge is in the max position"
				},
				{
					"name": "measureVal",
					"type": "number",
					"description": "The measure value"
				},
				{
					"name": "angleMin",
					"type": "number",
					"description": "The angle of the gauge at minimum value"
				},
				{
					"name": "angleMax",
					"type": "number",
					"description": "The angle of the gauge at maximum value"
				}
			],
            "body": "this.measureMin = measureMin;this.measureMax = measureMax;this.measureVal = measureVal;"
		}
	},
	"events": {
	   "onClick": {
			"description": "Gauge was clicked"
	   }
	}
